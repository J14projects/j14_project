import requests
from bs4 import BeautifulSoup

url = "https://www.family.co.jp/goods/newgoods.html"

response = requests.get(url)
soup = BeautifulSoup(response.content, "html.parser")

for product in soup.select(".newGoodsList li"):
    name = product.select_one(".newGoodsListName").text.strip()
    price = product.select_one(".newGoodsListPrice").text.strip()
    print(f"{name}: {price}")